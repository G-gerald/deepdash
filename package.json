{
  "name": "deepdash",
  "version": "2.1.3",
  "description": "➔ 𝐃eep extension for 𝐋odash: ✓ eachDeep ✓ filterDeep ✓ pickDeep ✓ omitDeep ✓ keysDeep ✓ indexate ✓ condenseDeep ⋮ Parent nodes tracking ⋮ Circular references check ⋮ Leaves only mode ⋮ Path as a valid js string or an array ⋮",
  "main": "deepdash.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "npm run test",
    "test": "nyc mocha  --reporter spec",
    "report": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon ./node_modules/.bin/mocha --reporter min --no-deprecation"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuriGor/deepdash.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "lodash",
    "each deep",
    "filter deep",
    "omit deep",
    "keys deep",
    "tree",
    "traversal",
    "eachDeep",
    "forEachDeep",
    "filterDeep",
    "pickDeep",
    "omitDeep",
    "keysDeep",
    "paths"
  ],
  "author": "Yuri Gor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YuriGor/deepdash/issues"
  },
  "homepage": "https://deepdash.io/",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "chai": "*",
    "chai-asserttype": "*",
    "coveralls": "*",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.3.1",
    "mocha": "*",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
