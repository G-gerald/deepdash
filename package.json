{
  "name": "deepdash",
  "version": "1.6.6",
  "description": "Object tree traversal for lodash",
  "main": "deepdash.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "npm run test",
    "test": "nyc mocha  --reporter spec",
    "report": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon ./node_modules/.bin/mocha --reporter min --no-deprecation"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuriGor/deepdash.git"
  },
  "keywords": [
    "lodash",
    "each deep",
    "filter deep",
    "keys deep",
    "object",
    "tree",
    "traversal",
    "eachDeep",
    "forEachDeep",
    "filterDeep",
    "keysDeep",
    "paths"
  ],
  "author": "Yuri Gor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YuriGor/deepdash/issues"
  },
  "homepage": "https://github.com/YuriGor/deepdash#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-asserttype": "^1.0.5",
    "coveralls": "^3.0.2",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
