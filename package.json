{
  "name": "deepdash",
  "version": "3.1.3",
  "description": "➔ 𝐃eep extension for 𝐋odash: ✓ eachDeep ✓ filterDeep ✓ pickDeep ✓ omitDeep ✓ keysDeep ✓ index ✓ condenseDeep ⋮ Parent nodes tracking ⋮ Circular references check ⋮ Leaves only mode ⋮ Path as a valid js string or an array ⋮",
  "main": "deepdash.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "npm run test",
    "test": "nyc mocha  --reporter spec",
    "report": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon ./node_modules/.bin/mocha -g 'pickDeep no mutation' --reporter min --no-deprecation",
    "debug": "node --inspect-brk test/debugDeep.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuriGor/deepdash.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "lodash",
    "each deep",
    "filter deep",
    "omit deep",
    "keys deep",
    "tree",
    "traversal",
    "eachDeep",
    "forEachDeep",
    "filterDeep",
    "pickDeep",
    "omitDeep",
    "keysDeep",
    "paths"
  ],
  "author": "Yuri Gor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YuriGor/deepdash/issues"
  },
  "homepage": "https://deepdash.io/",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "chai": "*",
    "chai-asserttype": "*",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "prettier": "^1.17.0"
  }
}
